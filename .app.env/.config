APP_ENV_CONFIG_ENCODING=utf8
APP_ENV_CONFIG_HANDLER_SEPARATOR=|
APP_ENV_CONFIG_VAR_WORD_SEPARATOR=_
# relative path from root folder
APP_ENV_CONFIG_DIST_FILE_PATH=./.env
APP_ENV_CONFIG_ENV_ROOT=./.app.env/
APP_ENV_CONFIG_INCLUDES_SEPARATOR=,
NODE_ENV=production
NODE_DEBUG=true
APP_ENV_CONFIG_MISSABLE=environment.processors.json:["APP_TEST_E2E_PROTRACTOR_DIRECT_CONNECT","APP_TEST_E2E_PROTRACTOR_BASE_URL","APP_TEST_E2E_PROTRACTOR_SELENIUM_ADDRESS"]
# relative paths from root folder separated by 'APP_ENV_CONFIG_INCLUDES_SEPARATOR'
APP_ENV_CONFIG_FILE_INCLUDES_KEY=--includes
# used to include files after everything is loaded to overload values using environment variable APP_PROCESS_FORCE_ENV_INCLUDES passed with the command separated by APP_ENV_CONFIG_INCLUDES_SEPARATOR
APP_ENV_CONFIG_PROCESS_FILE_INCLUDES_KEY=APP_PROCESS_FORCE_ENV_INCLUDES
--includes=./.app.env/.config.local
