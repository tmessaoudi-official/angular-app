{
    "root": {
        "allowed": [],
        "disallowed": ["master", "develop"]
    },
    "flow": {
        "separator": "/",
        "prefix": ["feature", "fix", "hotfix", "release"],
        "suffix": {
            "regex": "(SPARAC\\-\\d{1,5})"
        },
        "range": [7, 18]
    },
    "scripts": [
      ["Unit tests (Selenium)", ["make", ["test-unit-selenium"]]],
      ["E2e tests (Selenium)", ["make", ["test-e2e-selenium-build"]]]
    ],
    "skipMerge": true
}
