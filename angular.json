{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "cli": {
    "packageManager": "pnpm"
  },
  "projects": {
    "localization": {
      "i18n": {
        "sourceLocale": "home",
        "locales": {
          "en": "i18n/messages.en.xlf2",
          "fr": "i18n/messages.fr.xlf2",
          "de": "i18n/messages.de.xlf2",
          "es": "i18n/messages.es.xlf2",
          "ar": "i18n/messages.ar.xlf2",
          "pt": "i18n/messages.pt.xlf2",
          "it": "i18n/messages.it.xlf2"
        }
      },
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "webpack.config.js"
            },
            "allowedCommonJsDependencies": [
              "rxjs-compat",
              "rxjs"
            ],
            "i18nMissingTranslation": "error",
            "aot": true,
            "deleteOutputPath": false,
            "extractCss": true,
            "extractLicenses": true,
            "sourceMap": false,
            "vendorChunk": false,
            "commonChunk": false,
            "namedChunks": false,
            "buildOptimizer": false,
            "optimization": false,
            "outputHashing": "none",
            "outputPath": "dist/",
            "index": "src/index.html",
            "main": "src/main.ts",
            "i18nFormat": "xlf2",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig/tsconfig.app.json",
            "budgets": [
              {
                "type": "initial",
                "maximumWarning": "2mb",
                "maximumError": "5mb"
              },
              {
                "type": "anyComponentStyle",
                "maximumWarning": "6kb",
                "maximumError": "10kb"
              }
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ]
          },
          "configurations": {
            "production": {
              "buildOptimizer": true,
              "optimization": true,
              "outputHashing": "all",
              "namedChunks": false,
              "vendorChunk": false,
              "commonChunk": false,
              "sourceMap": false
            },
            "dev": {
              "buildOptimizer": false,
              "optimization": false,
              "outputHashing": "none",
              "namedChunks": true,
              "vendorChunk": true,
              "commonChunk": true,
              "sourceMap": true
            },
            "delete-output-path": {
              "deleteOutputPath": true
            },
            "localize": {
              "localize": true
            },
            "format": {
              "format": "xlf2"
            },
            "aot": {
              "aot": true
            },
            "no-aot": {
              "aot": false
            },
            "watch": {
              "watch": true
            },
            "no-base-href": {
              "baseHref": "",
              "outputPath": "dist"
            },
            "ignore-i18n": {
              "i18nMissingTranslation": "warning"
            },
            "experimental-rollup-pass": {
              "experimentalRollupPass": true
            },
            "dummy-env": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dummy.ts"
                }
              ]
            },
            "home": {
              "outputPath": "dist",
              "baseHref": ""
            },
            "home-base-href": {
              "outputPath": "dist/home",
              "baseHref": "/home/"
            },
            "home-locale": {
              "i18nFile": "i18n/messages.home.xlf2",
              "i18nLocale": "en"
            },
            "home-locale-en": {
              "i18nFile": "i18n/home/messages.en.xlf2",
              "i18nLocale": "en"
            },
            "home-en": {
              "i18nFile": "i18n/messages.en.xlf2",
              "i18nLocale": "en"
            },
            "home-locale-fr": {
              "i18nFile": "i18n/home/messages.fr.xlf2",
              "i18nLocale": "fr"
            },
            "home-fr": {
              "i18nFile": "i18n/messages.fr.xlf2",
              "i18nLocale": "fr"
            },
            "home-locale-ar": {
              "i18nFile": "i18n/home/messages.ar.xlf2",
              "i18nLocale": "ar"
            },
            "home-ar": {
              "i18nFile": "i18n/messages.ar.xlf2",
              "i18nLocale": "ar"
            },
            "home-locale-de": {
              "i18nFile": "i18n/home/messages.de.xlf2",
              "i18nLocale": "de"
            },
            "home-de": {
              "i18nFile": "i18n/messages.de.xlf2",
              "i18nLocale": "de"
            },
            "home-locale-es": {
              "i18nFile": "i18n/home/messages.es.xlf2",
              "i18nLocale": "es"
            },
            "home-es": {
              "i18nFile": "i18n/messages.es.xlf2",
              "i18nLocale": "es"
            },
            "home-locale-pt": {
              "i18nFile": "i18n/home/messages.pt.xlf2",
              "i18nLocale": "pt"
            },
            "home-pt": {
              "i18nFile": "i18n/messages.pt.xlf2",
              "i18nLocale": "pt"
            },
            "home-locale-it": {
              "i18nFile": "i18n/home/messages.it.xlf2",
              "i18nLocale": "it"
            },
            "home-it": {
              "i18nFile": "i18n/messages.it.xlf2",
              "i18nLocale": "it"
            },
            "home-config": {
              "tsConfig": "tsconfig/tsconfig.home.json",
              "main": "src/i18n/main/main.home.ts"
            },
            "en": {
              "outputPath": "dist",
              "baseHref": ""
            },
            "en-base-href": {
              "outputPath": "dist/en",
              "baseHref": "/en/"
            },
            "en-locale": {
              "i18nFile": "i18n/messages.en.xlf2",
              "i18nLocale": "en"
            },
            "fr": {
              "outputPath": "dist",
              "baseHref": ""
            },
            "fr-base-href": {
              "outputPath": "dist/fr",
              "baseHref": "/fr/"
            },
            "fr-locale": {
              "i18nFile": "i18n/messages.fr.xlf2",
              "i18nLocale": "fr"
            },
            "de": {
              "outputPath": "dist",
              "baseHref": ""
            },
            "de-base-href": {
              "outputPath": "dist/de",
              "baseHref": "/de/"
            },
            "de-locale": {
              "i18nFile": "i18n/messages.de.xlf2",
              "i18nLocale": "de"
            },
            "es": {
              "outputPath": "dist",
              "baseHref": ""
            },
            "es-base-href": {
              "outputPath": "dist/es",
              "baseHref": "/es/"
            },
            "es-locale": {
              "i18nFile": "i18n/messages.es.xlf2",
              "i18nLocale": "es"
            },
            "ar": {
              "outputPath": "dist",
              "baseHref": ""
            },
            "ar-base-href": {
              "outputPath": "dist/ar",
              "baseHref": "/ar/"
            },
            "ar-locale": {
              "i18nFile": "i18n/messages.ar.xlf2",
              "i18nLocale": "ar"
            },
            "pt": {
              "outputPath": "dist",
              "baseHref": ""
            },
            "pt-base-href": {
              "outputPath": "dist/pt",
              "baseHref": "/pt/"
            },
            "pt-locale": {
              "i18nFile": "i18n/messages.pt.xlf2",
              "i18nLocale": "pt"
            },
            "it": {
              "outputPath": "dist",
              "baseHref": ""
            },
            "it-base-href": {
              "outputPath": "dist/it",
              "baseHref": "/it/"
            },
            "it-locale": {
              "i18nFile": "i18n/messages.it.xlf2",
              "i18nLocale": "it"
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "customWebpackConfig": {
              "path": "webpack.config.js"
            },
            "liveReload": true,
            "browserTarget": "localization:build:dev,ignore-i18n,home-locale,home,no-base-href,home-config,aot"
          },
          "configurations": {
            "open": {
              "open": true
            },
            "dev": {
              "browserTarget": "localization:build:dev,ignore-i18n,home-locale,home,no-base-href,aot"
            },
            "home": {
              "browserTarget": "localization:build:dev,ignore-i18n,home-locale,home,no-base-href,home-config,aot"
            },
            "en": {
              "browserTarget": "localization:build:dev,ignore-i18n,en-locale,en,no-base-href,aot"
            },
            "fr": {
              "browserTarget": "localization:build:dev,ignore-i18n,fr-locale,fr,no-base-href,aot"
            },
            "de": {
              "browserTarget": "localization:build::dev,ignore-i18n,fr-locale,fr,no-base-href,aot"
            },
            "es": {
              "browserTarget": "localization:build:dev,ignore-i18n,es-locale,es,no-base-href,aot"
            },
            "ar": {
              "browserTarget": "localization:build:dev,ignore-i18n,ar-locale,ar,no-base-href,aot"
            },
            "pt": {
              "browserTarget": "localization:build:dev,ignore-i18n,pt-locale,pt,no-base-href,aot"
            },
            "it": {
              "browserTarget": "localization:build:dev,ignore-i18n,it-locale,it,no-base-href,aot"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "localization:build:home",
            "outputPath": "i18n",
            "outFile": "messages.xlf2",
            "format": "xlf2",
            "ivy": true
          },
          "configurations": {
            "source": {
              "browserTarget": "localization:build:home",
              "outFile": "messages.xlf2"
            },
            "home": {
              "browserTarget": "localization:build:home,home-config,dummy-env",
              "outputPath": "i18n/home",
              "outFile": "messages.xlf2"
            },
            "home-en": {
              "browserTarget": "localization:build:home,home-config,dummy-env,home-locale-en",
              "outputPath": "i18n/home",
              "outFile": "messages.en.xlf2"
            },
            "home-fr": {
              "browserTarget": "localization:build:home,home-config,dummy-env,home-locale-fr",
              "outputPath": "i18n/home",
              "outFile": "messages.fr.xlf2"
            },
            "home-ar": {
              "browserTarget": "localization:build:home,home-config,dummy-env,home-locale-ar",
              "outputPath": "i18n/home",
              "outFile": "messages.ar.xlf2"
            },
            "home-de": {
              "browserTarget": "localization:build:home,home-config,dummy-env,home-locale-de",
              "outputPath": "i18n/home",
              "outFile": "messages.de.xlf2"
            },
            "home-es": {
              "browserTarget": "localization:build:home,home-config,dummy-env,home-locale-es",
              "outputPath": "i18n/home",
              "outFile": "messages.es.xlf2"
            },
            "home-pt": {
              "browserTarget": "localization:build:home,home-config,dummy-env,home-locale-pt",
              "outputPath": "i18n/home",
              "outFile": "messages.pt.xlf2"
            },
            "home-it": {
              "browserTarget": "localization:build:home,home-config,dummy-env,home-locale-it",
              "outputPath": "i18n/home",
              "outFile": "messages.it.xlf2"
            },
            "en": {
              "browserTarget": "localization:build:en",
              "outFile": "messages.en.xlf2"
            },
            "fr": {
              "browserTarget": "localization:build:fr",
              "outFile": "messages.fr.xlf2"
            },
            "de": {
              "browserTarget": "localization:build:de",
              "outFile": "messages.de.xlf2"
            },
            "es": {
              "browserTarget": "localization:build:es",
              "outFile": "messages.es.xlf2"
            },
            "ar": {
              "browserTarget": "localization:build:ar",
              "outFile": "messages.ar.xlf2"
            },
            "pt": {
              "browserTarget": "localization:build:pt",
              "outFile": "messages.pt.xlf2"
            },
            "it": {
              "browserTarget": "localization:build:it",
              "outFile": "messages.it.xlf2"
            }
          }
        },
        "test": {
          "builder": "@angular-builders/custom-webpack:karma",
          "options": {
            "customWebpackConfig": {
              "path": "./webpack.config.js"
            },
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "codeCoverage": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.component.html"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js"
          },
          "configurations": {
            "selenium": {},
            "dev": {
              "devServerTarget": "localization:serve:dev"
            },
            "home": {
              "devServerTarget": "localization:serve:home"
            },
            "en": {
              "devServerTarget": "localization:serve:en"
            },
            "fr": {
              "devServerTarget": "localization:serve:fr"
            },
            "de": {
              "devServerTarget": "localization:serve:de"
            },
            "es": {
              "devServerTarget": "localization:serve:es"
            },
            "ar": {
              "devServerTarget": "localization:serve:ar"
            },
            "pt": {
              "devServerTarget": "localization:serve:pt"
            },
            "it": {
              "devServerTarget": "localization:serve:it"
            }
          }
        },
        "xliffmerge": {
          "builder": "@ngx-i18nsupport/tooling:xliffmerge",
          "options": {
            "format": "xlf2",
            "i18nFormat": "xlf2",
            "xliffmergeOptions": {
              "i18nFormat": "xlf2",
              "format": "xlf2",
              "srcDir": "i18n",
              "genDir": "i18n",
              "defaultLanguage": "home",
              "languages": [
                "home",
                "en",
                "fr",
                "ar",
                "de",
                "es",
                "pt",
                "it"
              ]
            }
          }
        }
      }
    }
  },
  "defaultProject": "localization"
}
