{
  "name": "localization",
  "version": "0.0.5",
  "keywords": [
    "angular",
    "i18n"
  ],
  "private": true,
  "engines": {
    "node": ">=15.2",
    "npm": ">=7.0"
  },
  "engineStrict": true,
  "maintainers": [
    {
      "name": "Takieddine Messaoudi",
      "email": "tmessaoudi@smart-team.tn"
    }
  ],
  "scripts": {
    "ng": "ng",
    "load-env-js": "APP_ENV_RUN=true ts-node ./node_modules/@tmessaoudi-official/dot-env-loader/dot-env-loader.run.js",
    "build": "ng build ${APP_CONFIGURATION}",
    "serve": "ng serve ${APP_CONFIGURATION}",
    "start-home": "APP_PROCESS_ENV_INCLUDES=./.app.env/.env.i18n.serve ng serve --configuration=home,disable-host-check",
    "start-en": "APP_PROCESS_ENV_INCLUDES=./.app.env/.env.i18n.serve ng serve --configuration=en,disable-host-check",
    "start-fr": "APP_PROCESS_ENV_INCLUDES=./.app.env/.env.i18n.serve ng serve --configuration=fr,disable-host-check",
    "start-ar": "APP_PROCESS_ENV_INCLUDES=./.app.env/.env.i18n.serve ng serve --configuration=ar,disable-host-check",
    "start-dummy": "APP_PROCESS_ENV_INCLUDES=./.app.env/.env.i18n.serve ng serve --configuration=dummy,disable-host-check",
    "start-home-en": "APP_PROCESS_ENV_INCLUDES=./.app.env/.env.i18n.serve ng serve --configuration=home-en,disable-host-check",
    "start-home-fr": "APP_PROCESS_ENV_INCLUDES=./.app.env/.env.i18n.serve ng serve --configuration=home-fr,disable-host-check",
    "start-home-ar": "APP_PROCESS_ENV_INCLUDES=./.app.env/.env.i18n.serve ng serve --configuration=home-ar,disable-host-check",
    "test": "ng test",
    "e2e": "ng e2e ${APP_CONFIGURATION}",
    "lint": "ng lint",
    "fix": "ng lint --fix",
    "extract-i18n": "ng extract-i18n --configuration=source && ng run localization:xliffmerge:app && ng extract-i18n --configuration=home-source && ng run localization:xliffmerge:home"
  },
  "dependencies": {
    "@angular/animations": "^11.0.2",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/forms": "^11.0.2",
    "@angular/localize": "^11.0.2",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@angular/router": "^11.0.2",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "@popperjs/core": "^2.5.4",
    "@types/crypto-js": "^4.0.1",
    "bootstrap": "^4.5.3",
    "crypto-js": "^4.0.0",
    "intl": "^1.2.5",
    "jquery": "^3.5.1",
    "ngx-toastr": "^13.1.0",
    "rxjs": "^6.6.3",
    "rxjs-compat": "^6.6.3",
    "systemjs": "^6.8.0",
    "tslib": "^2.0.3",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-builders/dev-server": "^7.3.3",
    "@angular-devkit/architect": "^0.1100.2",
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular-devkit/schematics": "^11.0.2",
    "@angular-eslint/builder": "^0.8.0-beta.3",
    "@angular-eslint/eslint-plugin": "^0.8.0-beta.3",
    "@angular-eslint/eslint-plugin-template": "^0.8.0-beta.3",
    "@angular-eslint/schematics": "^0.8.0-beta.3",
    "@angular-eslint/template-parser": "^0.8.0-beta.3",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@pnpm/logger": "^3.2.3",
    "@pnpm/pnpmfile": "^0.1.20",
    "@tmessaoudi-official/dot-env-loader": "https://github.com/tmessaoudi-official/dot-env-loader/tarball/0.0.9",
    "@types/jasmine": "^3.6.2",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.10",
    "@types/webpack": "^4.41.25",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/eslint-plugin-tslint": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-selenium-webdriver-launcher": "^0.0.4",
    "ngx-build-plus": "^10.1.1",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "protractor": "^7.0.0",
    "ts-node": "^9.0.0",
    "typescript": "~4.0.5"
  },
  "pnpm": {
    "overrides": {
      "fsevents": "^2.0.0",
      "chokidar": "^3.0.0",
      "@types/webpack": "^4.41.25",
      "@types/webpack-dev-server": "^3.11.1",
      "@schematics/angular": "^11.0.2",
      "@angular-devkit/schematics": "^11.0.2",
      "@angular-devkit/core": "^11.0.2",
      "@angular-devkit/build-angular": "^0.1100.2",
      "@angular-devkit/architect": "^0.1100.2",
      "typescript": "^4.0.5",
      "@types/node": "^14.4.10"
    },
    "replacements": {}
  }
}
