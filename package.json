{
  "name": "localization",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "load-env": "node ./webpack/dotenv/Loader.js",
    "serve": "ng serve ${APP_CONFIGURATION}",
    "build": "ng build ${APP_CONFIGURATION}",
    "xi18n": "ng xi18n ${APP_CONFIGURATION}",
    "xi18n-all": "APP_CONFIGURATION='--configuration=home' pnpm run xi18n | APP_CONFIGURATION='--configuration=en' pnpm run xi18n | APP_CONFIGURATION='--configuration=fr' pnpm run xi18n | APP_CONFIGURATION='--configuration=ar' pnpm run xi18n | APP_CONFIGURATION='--configuration=es' pnpm run xi18n | APP_CONFIGURATION='--configuration=de' pnpm run xi18n | APP_CONFIGURATION='--configuration=pt' pnpm run xi18n | APP_CONFIGURATION='--configuration=it' pnpm run xi18n",
    "lint": "pnpm run tslint && pnpm run eslint",
    "eslint": "ng run localization:eslint${APP_CONFIGURATION}",
    "tslint": "ng run localization:tslint${APP_CONFIGURATION}",
    "test": "ng test",
    "e2e": "ng e2e ${APP_CONFIGURATION}"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/localize": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "bootstrap": "^4.5.3",
    "es6-shim": "^0.35.6",
    "es6-symbol": "^3.1.3",
    "intl": "^1.2.5",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "rxjs": "~6.6.0",
    "rxjs-compat": "^6.6.3",
    "systemjs": "^6.7.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-builders/dev-server": "^7.3.1",
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular-eslint/builder": "^0.6.0-beta.0",
    "@angular-eslint/eslint-plugin": "^0.6.0-beta.0",
    "@angular-eslint/eslint-plugin-template": "^0.6.0-beta.0",
    "@angular-eslint/schematics": "^0.6.0-beta.0",
    "@angular-eslint/template-parser": "^0.6.0-beta.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@angular/localize": "^10.2.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "codelyzer": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.7",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
