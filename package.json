{
  "name": "localization",
  "version": "0.0.4",
  "keywords": [
    "angular",
    "i18n"
  ],
  "scripts": {
    "ng": "ng",
    "load-env-js": "ts-node ./webpack/dotenv/Loader.js",
    "load-env-ts": "ts-node ./src/DotEnv/loader.ts",
    "serve": "ng serve ${APP_CONFIGURATION}",
    "build": "ng build ${APP_CONFIGURATION}",
    "extract-i18n": "ng extract-i18n --configuration=source && ng run localization:xliffmerge:app && ng extract-i18n --configuration=home-source && ng run localization:xliffmerge:home",
    "lint": "ng lint",
    "test": "ng test",
    "e2e": "ng e2e ${APP_CONFIGURATION}",
    "start-home": "ng serve --configuration=home",
    "start-en": "ng serve --configuration=en",
    "start-fr": "ng serve --configuration=fr",
    "start-ar": "ng serve --configuration=ar",
    "start-de": "ng serve --configuration=de",
    "start-es": "ng serve --configuration=es",
    "start-pt": "ng serve --configuration=pt",
    "start-it": "ng serve --configuration=it",
    "start-dummy": "ng serve --configuration=dummy",
    "start-home-en": "ng serve --configuration=home-en",
    "start-home-fr": "ng serve --configuration=home-fr",
    "start-home-ar": "ng serve --configuration=home-ar",
    "start-home-de": "ng serve --configuration=home-de",
    "start-home-es": "ng serve --configuration=home-es",
    "start-home-pt": "ng serve --configuration=home-pt",
    "start-home-it": "ng serve --configuration=home-it"
  },
  "types": "module",
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.0",
    "@angular/common": "~11.0.0",
    "@angular/compiler": "~11.0.0",
    "@angular/core": "~11.0.0",
    "@angular/forms": "~11.0.0",
    "@angular/localize": "~11.0.0",
    "@angular/platform-browser": "~11.0.0",
    "@angular/platform-browser-dynamic": "~11.0.0",
    "@angular/router": "~11.0.0",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "bootstrap": "^4.5.3",
    "intl": "^1.2.5",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "rxjs": "~6.6.3",
    "rxjs-compat": "^6.6.3",
    "systemjs": "^6.7.1",
    "tslib": "^2.0.3",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-builders/dev-server": "^7.3.1",
    "@angular-devkit/build-angular": "^0.1100.1",
    "@angular-eslint/builder": "^0.6.0-beta.0",
    "@angular-eslint/eslint-plugin": "^0.6.0-beta.0",
    "@angular-eslint/eslint-plugin-template": "^0.6.0-beta.0",
    "@angular-eslint/schematics": "^0.6.0-beta.0",
    "@angular-eslint/template-parser": "^0.6.0-beta.0",
    "@angular/cli": "~11.0.1",
    "@angular/compiler-cli": "~11.0.0",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@pnpm/pnpmfile": "^0.1.20",
    "@types/jasmine": "~3.6.1",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.7",
    "@types/webpack": "^4.41.25",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "codelyzer": "^6.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ngx-build-plus": "^10.1.1",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "typescript": "~4.0.5"
  },
  "engines": {
    "node": ">=15.2",
    "npm": ">=7.0",
    "pnpm": ">=5.11",
    "ts-node": ">=9.0",
    "concurrently": ">=5.3"
  },
  "engineStrict": true
}
