    "karma-phantomjs-launcher": "^1.0.4",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1",
    "typescript": "~4.0.2"
  },
  "engines": {
    "node": ">=15.1",
    "pnpm": ">=5.11",
    "npm": ">=6.14"
  }
